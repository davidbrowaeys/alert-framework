<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aggregate_Function_Limit_Per_Alert</fullName>
    <active>true</active>
    <errorConditionFormula>Alert__r.Aggregate_Count__c &gt;= 1</errorConditionFormula>
    <errorMessage>In order to control performance, you cannot have more than one aggregate function per alert.</errorMessage>
</ValidationRule>
